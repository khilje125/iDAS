@using System.Web.Optimization
@{
    ViewBag.Title = "Merchant Account Info";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Hala Payment Service! | </title>
    <!-- Bootstrap -->
    <link href="../vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome -->
    <link href="../Content/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Animate.css -->
    <link href="https://colorlib.com/polygon/gentelella/css/animate.min.css" rel="stylesheet">
    <!-- Custom Theme Style -->
    <link href="../Content/css/iPaymentService.min.css" rel="stylesheet">
    <script src="~/Scripts/jquery-2.2.3.min.js"></script>
    <script src="~/vendors/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/vendors/validator/validator.min.js"></script>

    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
</head>
<body class="login">
    <div>
        <a class="hiddenanchor" id="signup"></a>
        <a class="hiddenanchor" id="signin"></a>
        <div class="login_wrapper">
            <div class="animate form login_form">
                <section class="login_content">
                    <h1 class="warning"><i class="warning-sign">Warning !</i></h1>
                    <p class="warning">Your account is <strong>Blocked</strong> due to some reason. Please contact Administrator for further details. </p>
                    <br />
                    <p>Thanks you using our services </p>
                    <a href="@Url.Action("Login","User")">Click Here for Login</a>
                    <div class="clearfix"></div>
                    <div class="separator">

                        <div class="clearfix"></div>
                        <br />
                        <div>
                            <h1><i class="fa fa-paw"></i> Payment Service!</h1>
                            <p>All Rights Reserved. CM Technologies (Payment Service) &copy; @System.DateTime.Now.ToString("yyyy")</p>
                        </div>
                    </div>
                </section>
            </div>

        </div>

    </div>
</body>
</html>