@using System.Web.Optimization
@*@using NavigationRoutes*@
@model iDAS.Models.ModelStudent
@{
    ViewBag.Title = "Edit Student Information";
    Layout = null;
}

 <!-- bootstrap-daterangepicker -->
<script type="text/javascript">


    $(document).ready(function () {

        $('#single_cal2').daterangepicker({
            singleDatePicker: true,
            calender_style: "picker_2"
        }, function (start, end, label) {
            console.log(start.toISOString(), end.toISOString(), label);
        });
    });

</script>
<!-- /bootstrap-daterangepicker -->

<div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">

            <div class="x_content">
                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-3" for="first-name">
                            Registrtion # <span class="required">*</span>
                        </label>
                        <div class="col-md-3 col-sm-3 col-md-3">
                            @Html.DisplayFor(m => m.RegNo, new { @class = "form-control col-md-3 col-md-3", @type = "number", })

                        </div>
                    </div>
               
                      <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-3" for="first-name">
                            Computer Code<span class="required">*</span>
                        </label>
                        <div class="col-md-3 col-sm-3 col-md-3">
                            @Html.DisplayFor(m => m.ComputerCode, new { @class = "form-control col-md-3 col-md-3", @type = "number", })

                        </div>
                    </div>
                 
                    <div class="form-group">
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            @Html.DisplayFor(m => m.StudentName, new { @class = "form-control col-md-7 col-xs-12" })

                        </div>
                        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
                            Student Name <span class="required">*</span>
                        </label>
                        
                    </div>
                
                    <div class="form-group">
                       <label class="control-label col-md-3 col-sm-3 col-xs-12">
                            Father Name<span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            @Html.DisplayFor(m => m.FatherName, new { @class = "form-control col-md-7 col-xs-12" })
                        </div>
                    </div>
                   
                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Address<span class="required">*</span></label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            @Html.DisplayFor(m => m.Address, new { @class = "form-control col-md-7 col-xs-12" })
                        </div>
                    </div>
                  

                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">
                            Class <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                          
                            @Html.DisplayFor(x => x.StudentClass.ClassName, new { @class = "form-control col-md-7 col-xs-12" })
                        </div>
                    </div>
                
                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">
                            Section <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                           @Html.DisplayFor(x => x.StudentSection.SectionName, new { @class = "form-control col-md-7 col-xs-12" })
                        </div>
                    </div>
                  
                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">
                            Fee <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            @Html.DisplayFor(m => m.MonthlyFee, new { @class = "form-control col-md-7 col-xs-12" })
                        </div>
                    </div>
                   

                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">Date of Birth  <span class="required">*</span></label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                           @Html.DisplayFor(c=>c.DateOfBirth, new{@class= "form-control col-md-7 col-xs-12" })
                           
                        </div>
                    </div>
                  
                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">
                            Address: <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            @Html.DisplayFor(m => m.Address, new { @class = "form-control col-md-7 col-xs-12" })
                        </div>
                    </div>
                   
                    <div class="form-group">
