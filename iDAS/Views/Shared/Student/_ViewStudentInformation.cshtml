                        <div class="col-md-6 col-sm-6 col-xs-12">
                           @Html.DisplayFor(m => m.LeaveClass, new { @class = "form-control col-md-7 col-xs-12" })
                        </div>
                    </div>
                  
                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">
                            Leave Date <span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                           @Html.DisplayFor(m => m.LeaveDate, new { @class = "form-control col-md-7 col-xs-12" })
                        </div>
                    </div>
                
                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">
