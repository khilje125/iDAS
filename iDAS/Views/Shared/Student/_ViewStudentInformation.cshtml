                            @Html.DisplayFor(m => m.LeaveDues, new { @class = "form-control col-md-7 col-xs-12" })
                        </div>
                    </div>
                  
                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">
                            Reason<span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                           @Html.DisplayFor(m => m.Reason, new { @class = "form-control col-md-7 col-xs-12" })
                        </div>
                    </div>
                   
                    <div class="form-group">
                        <label class="control-label col-md-3 col-sm-3 col-xs-12">
                            Reference<span class="required">*</span>
                        </label>
                        <div class="col-md-6 col-sm-6 col-xs-12">
                            @Html.DisplayFor(m => m.Reference, new { @class = "form-control col-md-7 col-xs-12" })
                        </div>
